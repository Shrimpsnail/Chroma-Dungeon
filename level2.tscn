script/source = "extends Node2D

var offsetsx = [0,0]
var offsetsy = [0,-64]


var cyoffsetsy = [0,-48]

var counter = 0

@export var red: Node
@export var green: Node
@export var blue: Node

@export var red_player: Node
@export var green_player: Node
@export var blue_player: Node

@export var music: Node

var rnjesus = RandomNumberGenerator.new()

func _ready():
	
	$Audio.set_stream(load(\"res://sounds/kick.wav\"))
	$Audio.playing=true

func _input(event):
	if event.is_action_pressed(\"ui_cancel\"):
		get_tree().quit()
		
	if event.is_action_pressed(\"smash\"):
		
		get_node(\"../Camera2D\").trauma = 0.5
		
		var sound = rnjesus.randi_range(1,3)
		if sound == 1:
			$Audio.set_stream(load(\"res://sounds/smash1.wav\"))
		elif sound == 2:
			$Audio.set_stream(load(\"res://sounds/smash2.wav\"))
		else :
			$Audio.set_stream(load(\"res://sounds/smash3.wav\"))
		$Audio.playing=true
		
		if counter == offsetsx.size()-1:
			counter = 0
			
			
			
			var collision = red_player.move_and_collide(Vector2(0, 0.1))
			if collision:
				if collision.get_collider() == red:
					red_player.position -= Vector2(offsetsx[counter+1],offsetsy[counter+1]) 
					
			
			collision = blue_player.move_and_collide(Vector2(0, 0.1))
			if collision:
				if collision.get_collider() == blue:
					blue_player.position += Vector2(offsetsx[counter+1],cyoffsetsy[counter+1]) 
		
			
			collision = green_player.move_and_collide(Vector2(0, 0.1))
			if collision:
				if collision.get_collider() == green:
					green_player.position += Vector2(offsetsx[counter+1],cyoffsetsy[counter+1]) 
				
			red.position = Vector2(0,0)
			blue.position = Vector2(0,0)
			green.position = Vector2(0,0)
			
		else:
			counter += 1
			
			
			var collision = red_player.move_and_collide(Vector2(0, 0.1))
			if collision:
				if collision.get_collider() == red:
					red_player.position += Vector2(offsetsx[counter],offsetsy[counter])
					
			
			collision = blue_player.move_and_collide(Vector2(0, 0.1))
			if collision:
				if collision.get_collider() == blue:
					blue_player.position -= Vector2(offsetsx[counter],cyoffsetsy[counter])
			
			collision = green_player.move_and_collide(Vector2(0, 0.1))
			if collision:
				if collision.get_collider() == green:
					green_player.position -= Vector2(offsetsx[counter],cyoffsetsy[counter])
			
			red.position += Vector2(offsetsx[counter],offsetsy[counter])
			blue.position -= Vector2(offsetsx[counter],cyoffsetsy[counter])
			green.position -= Vector2(offsetsx[counter],cyoffsetsy[counter])
			
					
					
	if event.is_action_pressed(\"reset\"):
		Globals.musicProgress  = music.get_playback_position() 
		get_tree().reload_current_scene()

	if event.is_action_pressed(\"save\"):
		$Audio.set_stream(load(\"res://sounds/menu_start.wav\"))
		$Audio.playing=true
		Globals.hasSaved  = true
		
		Globals.redPos = red_player.position 
		Globals.greenPos = green_player.position 
		Globals.bluePos = blue_player.position 
		
		Globals.counter = counter
	
	if event.is_action_pressed(\"load\"):
		if Globals.hasSaved:
			$Audio.set_stream(load(\"res://sounds/menu_change.wav\"))
			$Audio.playing=true
			
			red_player.position = Globals.redPos
			green_player.position = Globals.greenPos 
			blue_player.position = Globals.bluePos 
			
			counter = Globals.counter
			if counter == 0:
				red.position = Vector2(0,0)
				blue.position = Vector2(0,0)
				green.position = Vector2(0,0)
			else:
				red.position = Vector2(offsetsx[counter],offsetsy[counter])
				blue.position = -Vector2(offsetsx[counter],cyoffsetsy[counter])
				green.position = -Vector2(offsetsx[counter],cyoffsetsy[counter])
"

[sub_resource type="GDScript" id="GDScript_y0vbk"]
script/source = "extends Node2D


@export var toggle1: Node
@export var toggle2: Node



@export var disabled_tiles: Node
@export var disabled_tiles2: Node

var toggled = false
var toggled2 = false


func _on_toggle_body_entered(body):
	if((body.is_in_group(\"Player\") or body.is_in_group(\"Pushable\")) and not toggled):
		
		toggle1.get_node(\"AnimatedSprite2D\").play(\"on\")
		
		disabled_tiles.position.x = 0
		
		$Audio.set_stream(load(\"res://sounds/switch.wav\"))
		$Audio.playing=true
		
		toggled = true

func _on_toggle_2_body_entered(body):
	if((body.is_in_group(\"Player\") or body.is_in_group(\"Pushable\")) and not toggled2):
		
		toggle2.get_node(\"AnimatedSprite2D\").play(\"on\")
		
		disabled_tiles2.position.x = 1000
		
		$Audio.set_stream(load(\"res://sounds/switch.wav\"))
		$Audio.playing=true
		
		toggled2 = true


"

[sub_resource type="GDScript" id="GDScript_8lkhj"]
script/source = "extends Node2D

@export var next_level = \"level2\"

@export var red_player: Node
@export var green_player: Node
@export var blue_player: Node

@export var music: Node

var red = false
var green = false
var blue = false

	
	
func check():
	
	if(red and blue and green):
		Globals.musicProgress  = music.get_playback_position() 
		var next_scene = load(\"res://\"+ next_level+\".tscn\").instantiate()
		get_tree().root.add_child(next_scene)
		get_tree().set_current_scene(next_scene)
		get_node(\"/root/lvl2\").free()
		

func _on_area_2d_body_entered(body):
	if(body == red_player):
		print(\"red\")
		red = true
		check()
		
	elif(body == green_player):
		print(\"green\")
		green = true
		check()
		
	elif(body == blue_player):
		print(\"blue\")
		blue = true
		check()


func _on_area_2d_body_exited(body):
	if(body == red_player):
		red = false
		
	elif(body == green_player):
		green = false
		
	elif(body == blue_player):
		blue = false
"

